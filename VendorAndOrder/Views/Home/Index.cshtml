@using VendorAndOrder.Models;

<h1>Welcome Pierre to your vendor and order tracker app</h1>
  @if (Model.Count == 0)
  {
    <p>There are no vendors in the list</p>
  }
<ul>
  @foreach (Vendor vendor in Model)
  {
    <li>@vendor.Description</li>
  }
</ul>
<a href="/vendor/new">Vendors</a>
<a href='/items'> See list of vendors</a>
